<p id="notice"><%= notice %></p>

<h1>Polizas</h1>
<h2>IP: <%= @ip %></h2>
<table>
  <thead>
    <tr>
      <th>Poliza a migrar</th>
      <th>Prima</th>
      <th>Fecha migracion</th>
      <th>Observacion</th>
      <th>Ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polizas.each do |poliza| %>
      <tr>
        <td><%= poliza.poliza_a_migrar %></td>
        <td><%= poliza.prima %></td>
        <td><%= poliza.fecha_migracion %></td>
        <td><%= poliza.observacion %></td>
        <td><%= poliza.ip %></td>
        <td><%= link_to 'Show', poliza %></td>
        <td><%= link_to 'Edit', edit_poliza_path(poliza) %></td>
        <td><%= link_to 'Destroy', poliza, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poliza', new_poliza_path %>
